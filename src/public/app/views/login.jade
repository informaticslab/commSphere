.container-fluid
    // Page Heading
    .row
        .header.col-md-10
            h2.page-header
                | Login  
            .div
                .well.col-md-10
                    form
                        .form-group
                            input.form-control(placeholder='Email' ng-model='email')
                        .form-group
                            input.form-control(type='password' placeholder='Password' ng-model='password')
                        button.btn.btn-primary(ng-click="login(email, password)") Login
